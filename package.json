  },
  "homepage": "https://github.com/wyliao76/boilerplate#readme",
  "dependencies": {
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-openapi-validator": "^4.13.8",
    "helmet": "^5.1.0",
    "ioredis": "^5.0.6",
